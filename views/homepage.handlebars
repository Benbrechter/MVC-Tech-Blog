{{#each posts}}
 <div class="card"> 
        <div class="card-header">  
      <a href="/post/{{id}}"> <h2>{{post_title}}</h2> </a> 
          <h3>{{format_date_and_time createdAt}}</h3> 
        <h3>{{user.name}}</h3>
        </div>
        <div class="post-content">
        <p >{{post}}</p>
    </div>
    </div>
    {{/each}}